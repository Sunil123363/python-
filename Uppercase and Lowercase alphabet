def upperAlpha():
    for c in range(65, 91):
        print(chr(c), end = ' ')
    print('')

# function to print lowercase alphabet
def lowerAlpha():
    for c in range(97, 122):
        print(chr(c), end = ' ')
    print('')

# print uppercase alphabet
print('Uppercase Alphabet:')
upperAlpha()

# print lowercase alphabet
print('Lowercase Alphabet:')
lowerAlpha()  
